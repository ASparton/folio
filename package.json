{
	"name": "folio",
	"private": true,
	"version": "0.1.0",
	"author": "Alexandre Sparton<sparton.alexandre@gmail.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ASparton/count-of-money.git"
	},
	"bugs": {
		"url": "https://github.com/ASparton/count-of-money/issues"
	},
	"homepage": "https://github.com/ASparton/count-of-money#readme",
	"scripts": {
		"setup": "npm i && npx husky install"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/github",
			[
				"@semantic-release/exec",
				{
					"publishCmd": "./publish.sh ${nextRelease.version}"
				}
			],
			[
				"@semantic-release/git",
				{
					"assets": [
						"package.json"
					],
					"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
				}
			]
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^9.2.5",
		"husky": "^8.0.3",
		"semantic-release": "^22.0.12"
	}
}
